# Step 1: Install necessary packages (only once per session)
install.packages("ggplot2")
install.packages("dplyr")
install.packages("readr")



# Step 2: Load libraries
library(ggplot2)
library(dplyr)
library(readr)

# Step 3: Load dataset
df <- read.csv("C:/Users/Supraja/Downloads/pricequotes.csv")
cat("Barry price summary")
cat(df$Barry.Price)
cat("Standard deviation:",sd(df$Barry.Price))
cat("Mary price summary:")
cat(df$Mary.Price)
print(summary(df))
boxplot(df$Barry.Price,df$Mary.Price,
        names=c("Barry","Mary"),
        col=c("skyblue","lightgreen"),
        main="Boxplot of Price Quotes",
        ylab="Price"
)
